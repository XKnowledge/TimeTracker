{
  "name": "TimeTracker",
  "version": "1.0.0",
  "description": "个人时间追踪网站",
  "main": "main.js",
  "author": "XKnowledge Team",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "build": {
    "appId": "com.TimeTracker.TimeTracker",
    "productName": "TimeTracker",
    "compression": "store",
    "publish": null,
    "directories": {
      "output": "release"
    },
    "asar": true,
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "!**/node_modules/**/*"
    ]
  },
  "devDependencies": {
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1"
  }
}
